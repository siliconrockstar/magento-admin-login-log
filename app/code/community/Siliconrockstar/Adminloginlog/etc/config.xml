<?xml version="1.0"?>
<config>
    <modules>
        <Siliconrockstar_Adminloginlog>
            <version>0.9.0.0</version>
        </Siliconrockstar_Adminloginlog>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Siliconrockstar_Adminloginlog after="Mage_Adminhtml">Siliconrockstar_Adminloginlog_Adminhtml</Siliconrockstar_Adminloginlog>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <global>
        <blocks>
            <siliconrockstar_adminloginlog>
                <class>Siliconrockstar_Adminloginlog_Block</class>
            </siliconrockstar_adminloginlog>
        </blocks>
        <helpers>
            <siliconrockstar_adminloginlog>
                <class>Siliconrockstar_Adminloginlog_Helper</class>
            </siliconrockstar_adminloginlog>
        </helpers>
        <models>
            <siliconrockstar_adminloginlog>
                <class>Siliconrockstar_Adminloginlog_Model</class>
                <resourceModel>siliconrockstar_adminloginlog_resource</resourceModel>
            </siliconrockstar_adminloginlog>
            <siliconrockstar_adminloginlog_resource>
                <class>Siliconrockstar_Adminloginlog_Model_Resource</class>
                <entities>		  
                    <log>
                        <table>siliconrockstar_adminloginlog_log</table>
                    </log>
                </entities>
            </siliconrockstar_adminloginlog_resource>
        </models>
        <events>
            <admin_session_user_login_success>
                <observers>
                    <admin_session_user_login_success_handler>
                        <type>model</type>
                        <class>siliconrockstar_adminloginlog/observer</class>
                        <method>onAdminLogin</method>
                        <args></args>
                    </admin_session_user_login_success_handler>
                </observers>
            </admin_session_user_login_success>
            <admin_session_user_login_failed>
                <observers>
                    <admin_session_user_login_failed_handler>
                        <type>model</type>
                        <class>siliconrockstar_adminloginlog/observer</class>
                        <method>onAdminLoginFailed</method>
                        <args></args>
                    </admin_session_user_login_failed_handler>
                </observers>
            </admin_session_user_login_failed>
        </events>
        <resources>
            <siliconrockstar_adminloginlog_setup>
                <setup>
                    <module>Siliconrockstar_Adminloginlog</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </siliconrockstar_adminloginlog_setup>
            <siliconrockstar_adminloginlog_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </siliconrockstar_adminloginlog_write>
            <siliconrockstar_adminloginlog_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </siliconrockstar_adminloginlog_read>
        </resources>
    </global>
</config>